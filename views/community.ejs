<!DOCTYPE html>
<html lang="en">
  <%- include("partials/head") -%>

  <body>
    <%- include("partials/navigation", {page:"communities"}); -%> <% if(communityData.length!=0){
    var i=1; communityData.forEach(function(data){ %>
    <div class="site-wrap bg-light">
      <div class="site-section">
        <div class="container">
          <div class="row">
            <div class="col-md-6">
              <img src="<%=data.CommunityImg%>" alt="Image" class="img-fluid" />
            </div>
            <div class="col-md-6 pt-3">
              <h2 class="text-black"><%=data.CommunityName %></h2>
              <p><%=data.Description %></p>
              <p>
                <a href="/communityform" class="buy-now btn btn-sm btn-primary">Request to join</a>
              </p>
            </div>
          </div>
        </div>
      </div>

      <% i++; }) %> <% } else{ %>
      <tr>
        <td colspan="7">No Data Found</td>
      </tr>
      <% } %>

      <!-- Start Fixed Background IMG -->

      <div class="fixed-background">
        <div class="fixed-wrap">
          <div class="fixed"></div>
        </div>
      </div>
    </div>

    <!-- Start Footer -->
    <footer>
      <div class="container">
        <div class="row text-light text-center py-4 justify-content-center">
          <div class="col-sm-10 col-md-8 col-lg-6">
            <h4>Find us on our Socials</h4>
            <ul class="social pt-3">
              <li>
                <a href="#" target="-blank"><i class="fab fa-facebook"></i></a>
              </li>
              <li>
                <a href="#" target="-blank"><i class="fab fa-twitter"></i></a>
              </li>
              <li>
                <a href="#" target="-blank"><i class="fab fa-instagram"></i></a>
              </li>
              <li>
                <a href="#" target="-blank"><i class="fab fa-youtube"></i></a>
              </li>
            </ul>
            <p class="pt-4">
              &copy;
              <a
                href="https://www.ibm.com/uk-en?p1=Search&p4=43700052448841159&p5=e&gclid=CjwKCAjwt8uGBhBAEiwAayu_9RiWGlIK-td-ZExRQ7Ez-MLdqCXJbzrivRuE1uDkvki01kXlk2TFpxoCG-EQAvD_BwE&gclsrc=aw.ds"
                target="_black"
                >www.ibm.com</a
              >
            </p>
          </div>
        </div>
      </div>
    </footer>

    <!-- End Footer -->

    <!-- jQuery -->
    <script src="js/jquery-3.5.1.min.js"></script>
    <!-- Bootstrap 4.5 JS -->
    <script src="js/bootstrap.min.js"></script>
    <!-- Popper JS -->
    <script src="js/popper.min.js"></script>
    <!-- Font Awesome -->
    <script src="js/all.min.js"></script>

    <script src="js/jquery-3.3.1.min.js"></script>
    <script src="js/jquery-ui.js"></script>
    <script src="js/popper.min.js"></script>
    <script src="js/bootstrap.min.js"></script>
    <script src="js/owl.carousel.min.js"></script>
    <script src="js/jquery.magnific-popup.min.js"></script>
    <script src="js/aos.js"></script>

    <script src="js/main.js"></script>

    <!-- End Script Source Files -->
  </body>
</html>
